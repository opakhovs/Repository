@{
    ViewBag.Title = "Home Page";
    Layout = "~/Views/Shared/_MainPageLayout.cshtml";
}

<div class="jumbotron">
    <h1>Repository</h1>
    <p class="lead">Repository is a free application to store all kinds of files, including binary ones. It includes anything from zipped up source codes, to build results, to things like docker containers as well.</p>
    <p><a href="https://assessment-tools.ca.com/tools/continuous-delivery-tools/en/artefact-repository-management" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
</div>

<div class="row">
    <div class="col-md-6">
        <h2>Getting started</h2>
        <p>
            Try to create your first artifact type!
        </p>
        @Html.ActionLink("Start", "Create", new { controller = "ArtTypes" }, new { @class = "btn btn-default" })
    </div>
    <div class="col-md-6">
        <h2>Get more rools</h2>
        <p>Check some tools related with software artifacts management</p>
        <p>
            <a href="http://repository20200321080742.azurewebsites.net/Admin/files/report.pdf"
               onClick="javascript:
               pageTracker._trackPageview('/downloads/report');">

                <a target="_blank" onClick="javascript: ga('myTracker.set', 'page', '/download');
        ga('myTracker.send', 'pageview');gtag('event', 'png', {'event_category' : '/download', 'event_label' : 'test.png'});"
                   class="btn btn-default" href="https://repository20200321080742.azurewebsites.net/ww">Learn more &raquo;</a>
        </p>
    </div>
